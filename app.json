{
  "expo": {
    "name": "PhotoBoom",
    "slug": "PhotoBoom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/photoboom_logo_1024.png",
    "splash": {
      "image": "./app/assets/splash_photoboom.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5f5",
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.group5.PhotoBoom",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/photoboom_logo.png"
    }
  }
}
